#include <stdio.h>
int main(void)
{
	int x, y, z;

	while(1)
	{
		scanf("%d %d %d", &x, &y, &z);

		if(x == 0 && y == 0 && z==0)
			break;

		if(x>y && x>z){
			if((z*z) + (y*y) == (x*x))
				printf("right\n");
			else
				printf("wrong\n");
		}
		else if(y>x && y>z){
			if((z*z) + (x*x) == (y*y))
				printf("right\n");
			else
				printf("wrong\n");
		}
		else{
			if((x*x) + (y*y) == (z*z))
				printf("right\n");
			else
				printf("wrong\n");
		}
	}
	return 0;
}
